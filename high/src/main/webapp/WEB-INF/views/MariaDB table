CREATE DATABASE hihigh;

USE hihigh;

CREATE TABLE MEMBER (
    id VARCHAR(50) NOT NULL PRIMARY KEY,
    pwd VARCHAR(30) NOT NULL,
    name VARCHAR(50) NOT NULL,
	age char(10) not NULL,
    tel VARCHAR(30),
    email VARCHAR(30),
	filename VARCHAR(30),
    reg_date DATE DEFAULT CURRENT_DATE,
);


