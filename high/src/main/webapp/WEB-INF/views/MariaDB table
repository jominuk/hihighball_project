UPDATE MEMBER
SET tel = CONCAT('010', LPAD(FLOOR(RAND() * 1000000000), 8, '0'));


CREATE TABLE board (
    bnum INT AUTO_INCREMENT PRIMARY KEY,
    id VARCHAR(30) NOT NULL,
    subject VARCHAR(100) NOT NULL,
    contents VARCHAR(1000) NOT NULL,
    postfile VARCHAR(30),
	profile VARCHAR(50),
    count INT DEFAULT 0,
    re_date TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
	 FOREIGN KEY (id) REFERENCES member(id) ON DELETE CASCADE
);


CREATE TABLE liky (
  id VARCHAR(20) NOT NULL ,
  bnum INT NOT NULL,
  state INT DEFAULT 0,
  PRIMARY KEY (id, bnum),
  FOREIGN KEY (id) REFERENCES member(id) ON DELETE CASCADE
);


CREATE TABLE reply (
    renum INT AUTO_INCREMENT PRIMARY KEY,
    bnum INT NOT NULL,
    id VARCHAR(20) NOT NULL,
    recontents VARCHAR(50) NOT NULL,
    redate TIMESTAMP DEFAULT CURRENT_TIMESTAMP,
    FOREIGN KEY (bnum) REFERENCES board(bnum) ON DELETE CASCADE,
    FOREIGN KEY (id) REFERENCES member(id) ON DELETE CASCADE
);


create table event(
  id varchar(15) not null comment'회원 id',
  company varchar(10) not null comment'회사',
  name varchar(10)  comment'이름',
  tel varchar(13)  comment'회원 번호',
  num varchar(5)  comment'인원',
  datech varchar(20)  comment'날짜',
  reservation char(10) DEFAULT '0',
  PRIMARY KEY(id, company),
  FOREIGN KEY (id) REFERENCES member (id) ON DELETE CASCADE
);



INSERT INTO EVENT (id, company, name, tel, num, datech)
SELECT
  M.id,
  CASE FLOOR(RAND() * 5)
    WHEN 0 THEN '발그레'
    WHEN 1 THEN '퍼지'
    WHEN 2 THEN '핌리코'
    WHEN 3 THEN '네셔널'
    WHEN 4 THEN '지오'
  END AS company,
  LEFT(M.name, 10),
  COALESCE(LPAD(M.tel, 11, '010'), '00000000000'),
  CAST(FLOOR(RAND() * 11) AS CHAR(2)),
  CURRENT_DATE
FROM
  MEMBER M
ORDER BY RAND()
LIMIT 190;












